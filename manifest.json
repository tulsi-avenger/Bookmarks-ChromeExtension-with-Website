{
	"manifest_version": 2,
	"name": "Hello World! ",
	"version": "1.0",
	"description": "my first chrome extension",
	"icons": {
		"16": "images/icon.png" ,
		"48": "images/48.png",
		"128": "images/128.png"
	},
	"browser_action" :{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions" : [
		"activeTab",
		"storage"
	]
}